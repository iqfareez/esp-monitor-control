// Menu button
const int menuButtonPin = D8;
int buttonPushCounter = 0; // counter for the number of button presses
int buttonState = 0;       // current state of the button
int lastButtonState = 0;   // previous state of the button

enum Menu
{
    MODE_BRIGHTNESS,
    MODE_CONTRAST,
    MODE_VOLUME,
};

Menu currentMenu = MODE_BRIGHTNESS;

void setup()
{
    Serial.begin(9600);

    // Setup OLED display
    display.setRotation(2); // https://learn.adafruit.com/adafruit-gfx-graphics-library/rotating-the-display
    display.begin(0x3C, true);
    display.setContrast(0x20); // dim display (0 -127)

    // show loading animation
    int frame = 0;
    for (int i = 0; i < 20; i++)
    {
        display.clearDisplay();
        display.drawBitmap(40, 8, frames[frame], FRAME_WIDTH, FRAME_HEIGHT, 1);
        display.display();
        frame = (frame + 1) % FRAME_COUNT;
        delay(FRAME_DELAY);
    }

    display.clearDisplay();

    // Init pinmodes
    pinMode(menuButtonPin, INPUT_PULLUP);

    // Display welcome text
    display.setTextSize(1);
    display.setTextColor(SH110X_WHITE);
    display.setCursor(0, 0);
    display.println("iqfareez is awesome");
    display.setTextSize(2);
    display.setTextColor(SH110X_WHITE);
    display.print("0x");
    display.println(0xDEADBEEF, HEX);
    display.display();
    delay(1000);
    display.clearDisplay();
}

void drawCentreString(const char *buf, int x, int y, int size)
{
    // First solution from: https://forum.arduino.cc/t/adafruit-oled-how-to-center-text/617181/6
    // It doesn't work well for text size more than 1
    // int16_t x1, y1;
    // uint16_t w, h;
    // display.setTextSize(size);
    // display.getTextBounds(buf, x, y, &x1, &y1, &w, &h); // calc width of new string
    // display.setCursor(x - w / 2, y);
    // display.print(buf);

    // Second approach, doesn't rely on getTextBounds. Seems to work well
    uint8_t charWidth = 6 * size; // Approximate width of each character
    uint8_t textLength = strlen(buf);
    int16_t totalWidth = charWidth * textLength;
    display.setTextSize(size);
    display.setCursor(x - totalWidth / 2, y);
    display.print(buf);
}

void loop()
{
    if (currentMenu == MODE_BRIGHTNESS)
    {
        display.setCursor(0, 0);
        display.setTextColor(SH110X_WHITE);
        // display.setTextSize(5);
        drawCentreString("78", 69, 12, 5);
        // display.println(F("Brightness"));
        drawCentreString("Brightness", 64, 50, 1);

        display.display();
    }
    else if (currentMenu == MODE_CONTRAST)
    {
        display.setCursor(0, 0);
        display.setTextColor(SH110X_WHITE);
        // display.setTextSize(5);
        drawCentreString("81", 69, 12, 5);
        // display.println(F("Brightness"));
        drawCentreString("Contrast", 64, 50, 1);

        display.display();
    }
    else if (currentMenu == MODE_VOLUME)
    {
        display.setCursor(0, 0);
        display.setTextColor(SH110X_WHITE);
        // display.setTextSize(5);
        drawCentreString("100", 69, 12, 5);
        // display.println(F("Brightness"));
        drawCentreString("Volume", 64, 50, 1);

        display.display();
    }
    else
    {
        display.setCursor(0, 0);
        display.setTextColor(SH110X_WHITE);
        drawCentreString("I'm in denial", 64, 50, 1);

        display.display();
    }

    // Button edge detection: https://www.arduino.cc/en/Tutorial/BuiltInExamples/StateChangeDetection
    // read the pushbutton input pin:
    buttonState = digitalRead(menuButtonPin);

    // compare the buttonState to its previous state
    if (buttonState != lastButtonState)
    {
        // if the state has changed, increment the counter
        if (buttonState == LOW)
        {
            // Change the menu state
            display.clearDisplay();
            if (currentMenu == MODE_BRIGHTNESS)
            {
                currentMenu = MODE_CONTRAST;
            }
            else if (currentMenu == MODE_CONTRAST)
            {
                currentMenu = MODE_VOLUME;
            }
            else if (currentMenu == MODE_VOLUME)
            {
                currentMenu = MODE_BRIGHTNESS;
            }
        }

        // Delay a little bit to avoid bouncing
        delay(50);
    }
    // save the current state as the last state, for next time through the loop
    lastButtonState = buttonState;
}